#include <iostream>
using namespace std;

int main()
{
    int n, a[100][100];
    cin >> n;
    for(int i = 0; i < n; i++)
    {
        for(int j = 0 ; j < n; j++)
        {
            cin>>a[i][j];
        }
    }
    int e =0;
    for(int i=0; i<n; i++) {
        for(int j=i+1; j<n; j++) {
            if(a[i][j] != a[j][i])
            {
                e++;
            }
            else if (a[i][i] != 0) {
                e++;
            }
            else if (a[i][j] != 0 && a[i][j] != 1) {
                e++;
            }
        }
    }
    if(e >= 1)
    {
        cout << "No" << endl;
    }
    else
    {
        cout << "Yes" << endl;
    }
    return 0;
}
