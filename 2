#include <iostream>
using namespace std;

int main()
{
    int n, a[100][100];
    cin >> n;
    for(int i = 0; i < n; i++)
    {
        for(int j = 0 ; j < n; j++)
        {
            cin>>a[i][j];
        }
    }
    int e = 0;
    for(int i=0; i<n; i++)
    {
        if(a[i][i] == 0)
        {
            e++;
        }
    }
    if(e >= 1)
    {
        cout << "Yes" << endl;
    }
    else
    {
        cout << "No" << endl;
    }
    return 0;
}
